{
  "problemMatcher": [
    {
      "owner": "cpp",
      "pattern": [
        {
          "regexp": "^(.*):(\\d+):(\\d+): (warning|error|fatal error): (.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5
        }
      ]
    },
    {
      "owner": "cmake",
      "pattern": [
        {
          "regexp": "^CMake (Warning|Error) at (.*):(\\d+) \\((.*)\\):$",
          "severity": 1,
          "file": 2,
          "line": 3,
          "code": 4
        },
        {
          "regexp": "^  (.*)$",
          "message": 0,
          "loop": true
        }
      ]
    },
    {
      "owner": "cmake_noorigin",
      "pattern": [
        {
          "regexp": "^CMake (Warning|Error):$",
          "severity": 1
        },
        {
          "regexp": "^  (.*)$",
          "message": 0,
          "loop": true
        }
      ]
    }
  ]
}
